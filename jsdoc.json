{
  "opts": {
    "template": "node_modules/better-docs",
    "readme": "readme.md",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": [
      "optional",
      "category"
    ]
  },
  "plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/category"
  ],
  "source": {
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "templates": {
    "search": true,
    "better-docs": {
      "name": "aa-js Documentation",
      "title": "aa-js",
      "css": "style.css",
      "hideGenerator": false,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/onekiloparsec/aa-js"
        }
      ]
    }
  }
}
