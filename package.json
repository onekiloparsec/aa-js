    "webpack-dev-server": "^2.4.0",
    "webpack-merge": "^4.0.0",
    "yarn-or-npm": "^2.0.0"
  },
  "dllPlugin": {
    "name": "pluginTemplateDeps",
    "include": [
      "html-entities",
      "vue/dist/vue.js",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
